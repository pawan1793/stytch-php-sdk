{
    "name": "pawanmore/stytch-php-sdk",
    "description": "UnOfficial PHP SDK for Stytch Authentication APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pawan More",
            "email": "hello@pawanmore.com"
        }
    ],
    "keywords": [
        "stytch",
        "b2b",
        "consumer",
        "authentication",
        "sso",
        "mfa",
        "oauth",
        "passwordless",
        "api",
        "php",
        "unofficial",
        "php sdk",
        "stytch sdk",
        "stytch php sdk"
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Stytch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stytch\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}